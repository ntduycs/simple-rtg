{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@apis/*": [
        "src/apis/*"
      ],
      "@app/*": [
        "src/app/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@common/*": [
        "src/common/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@features/*": [
        "src/features/*"
      ],
      "@layout/*": [
        "src/layout/*"
      ],
      "@type/*": [
        "src/type/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ]
    }
  }
}